{
    "openapi": "3.0.1",
    "info": {
        "title": "ICMS-Java 项目 API 文档",
        "description": "这是 ICMS-Java 项目的接口文档，描述项目相关功能接口",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://121.36.44.77:8090",
            "description": "Generated server url"
        }
    ],
    "tags": [
        {
            "name": "考勤管理接口",
            "description": "提供查看考勤功能"
        },
        {
            "name": "测试接口",
            "description": "提供各类测试接口，用于验证 Swagger 文档生成及接口测试"
        },
        {
            "name": "用户管理接口",
            "description": "提供用户注册、登录、获取当前用户、退出登录等功能接口"
        }
    ],
    "paths": {
        "/user/signin": {
            "post": {
                "tags": [
                    "用户管理接口"
                ],
                "summary": "用户签到接口",
                "description": "接收用户签到信息，验证通过后返回用户信息，签到失败返回对应提示",
                "operationId": "userSignin",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/UserSigninRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/register": {
            "post": {
                "tags": [
                    "用户管理接口"
                ],
                "summary": "用户注册接口",
                "description": "接收用户注册信息，完成用户注册操作，返回注册结果状态码",
                "operationId": "userRegister",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/UserRegisterRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "注册结果，1 表示成功，0 表示失败",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "integer",
                                    "example": 1
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "请求体为空或参数不合法",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "string",
                                    "example": "请求体不能为空"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/logout": {
            "post": {
                "tags": [
                    "用户管理接口"
                ],
                "summary": "用户退出登录接口",
                "description": "使当前用户会话失效，清除相关 Cookie，完成退出操作",
                "operationId": "logout",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/login": {
            "post": {
                "tags": [
                    "用户管理接口"
                ],
                "summary": "用户登录接口",
                "description": "接收用户登录信息，验证通过后返回用户信息，登录失败返回对应提示",
                "operationId": "userLogin",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/UserLoginRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/test/submit": {
            "post": {
                "tags": [
                    "测试接口"
                ],
                "summary": "提交测试数据接口",
                "description": "接收测试数据并进行处理，返回提交结果",
                "operationId": "submitTestData",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/TestRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "400": {
                        "description": "请求体为空或参数不合法",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "string",
                                    "example": "请求体不能为空"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "数据提交成功",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/user/current": {
            "get": {
                "tags": [
                    "用户管理接口"
                ],
                "summary": "获取当前用户接口",
                "description": "从会话中获取当前登录用户的用户名并返回",
                "operationId": "getCurrentUser",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/test/{id}": {
            "get": {
                "tags": [
                    "测试接口"
                ],
                "summary": "获取测试数据接口",
                "description": "根据传入的 ID，获取对应的测试数据",
                "operationId": "getTestData",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "测试数据的唯一标识 ID",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        },
                        "example": 1
                    }
                ],
                "responses": {
                    "400": {
                        "description": "参数不合法，ID 格式错误等",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "string",
                                    "example": "ID 必须为有效数字"
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "成功获取测试数据",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/test/reverse": {
            "get": {
                "tags": [
                    "test-contorller"
                ],
                "operationId": "reverseStringFromFastAPI",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/attendance/records": {
            "get": {
                "tags": [
                    "考勤管理接口"
                ],
                "summary": "返回考勤记录",
                "description": "获取用户考勤记录",
                "operationId": "attendanceRecords",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "*/*": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/AttendanceLogRequest"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "UserSigninRequest": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "faceEmbedding": {
                        "type": "string"
                    },
                    "faceImage": {
                        "type": "string"
                    }
                }
            },
            "UserRegisterRequest": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "checkPassword": {
                        "type": "string"
                    },
                    "faceEmbedding": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string"
                    }
                },
                "description": "注册信息（用户名、密码、确认密码）"
            },
            "UserLoginRequest": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "faceEmbedding": {
                        "type": "string"
                    }
                }
            },
            "TestRequest": {
                "type": "object",
                "properties": {
                    "message": {
                        "type": "string",
                        "description": "提交的消息内容",
                        "example": "测试提交内容"
                    }
                },
                "description": "提交的测试数据，包含 message 字段"
            },
            "User": {
                "type": "object",
                "properties": {
                    "userId": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "username": {
                        "type": "string"
                    },
                    "password": {
                        "type": "string"
                    },
                    "role": {
                        "type": "object"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updatedAt": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "faceEmbedding": {
                        "type": "string"
                    },
                    "sourceImageUrl": {
                        "type": "string"
                    }
                }
            },
            "AttendanceLogRequest": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "timestamp": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "faceImage": {
                        "type": "string"
                    }
                }
            },
            "2222": {
                "type": "object",
                "properties": {
                    "username": {
                        "type": "string"
                    },
                    "timestamp": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "faceImage": {
                        "type": "string"
                    }
                }
            }
        }
    }
}